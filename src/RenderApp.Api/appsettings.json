{
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "http://*:4901",
        "Protocols": "Http2"
      },
      "Http": {
        "Url": "http://*:4900",
        "Protocols": "Http1AndHttp2"
      }
    }
  },
  "logger": {
    "labels": {
      "application": "render-app",
      "environment": "test"
    },
    "filters": {
      "enabled": true,
      "filters": [
        {
          "type": "Exclude",
          "Expression": "SourceContext like 'Microsoft.EntityFrameworkCore%'"
        }
      ]
    },
    "console": {
      "enabled": true
    }
  }
}
